{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": [
          "./src"
        ],
        "extensions": [
          ".js",
          ".ts",
          ".tsx",
          ".ios.js",
          ".android.js"
        ],
        "alias": {
          "@actions": "./src/actions",
          "@components": "./src/components",
          "@constants": "./src/constants",
          "@images": "./src/images",
          "@metrics": "./src/metrics",
          "@navigators": "./src/navigators",
          "@reducers": "./src/reducers",
          "@screens": "./src/screens",
          "@services": "./src/services",
          "@store": "./src/store",
          "@theme": "./src/theme",
          "@styles": "./src/styles",
          "@types": "./src/types",
          "@utils": "./src/utils"
        }
      }
    ]
  ]
}